  }

  binaryOutput: {
    #      module_type     : BinaryPacketsFromDataBlocks
    module_type     : ArtBinaryPacketsFromDigis
    strawDigiCollection : "makeSD"
    caloDigiCollection  : "CaloDigiMaker"
    crvDigiCollection   : "CrvDigi"
