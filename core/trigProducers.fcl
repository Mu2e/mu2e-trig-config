    generateBinaryFile : 1
    generateTextFile   : 0
    #         outputFile      : "DTC_packets.bin"
    #         maxDMABlockSize : 32000
    #         bufferSize      : 1000
  }
}

TrigCalProducers : {
  CaloHitMakerFast:
  {
    module_type   : "CaloHitsFromFragments"
    diagLevel     : 0
    caloTag       : "artFragFromDTCEvents"
    digiSampling  : 5
    deltaTPulses  : 5.
    nPEperMeV     : 30
    noiseLevelMeV : 0.55
    nSigmaNoise   : 4
    # hitEDepMax    : 80.
    hitEDepMax    : 1000. # for testing
