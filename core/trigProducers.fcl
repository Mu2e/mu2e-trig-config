TrigTrkProducers : {
  TTmakeSH: {
    diagLevel: 0
    module_type: "StrawHitRecoFromFragments"
    printLevel : 0
    FitType : 5
    UseCalorimeter : false
    clusterDt : 100
    minimumEnergy : 0.0
    maximumEnergy : 0.0035
    minimumRho   : 0.
    maximumRho   : 800.
    crossTalkEnergy : 0.007
    crossTalkMinimumTime : -1
    crossTalkMaximumTime : 100
    minimumTime : 500
