   }
   TTmakePH: {
      @table::TrkHitReco.producers.makePH
      TestFlag   : false # not needed, since TTmakeSH is filtering
      FilterHits : false
      ComboHitCollection    : "TTmakeSH"
      StrawHitSelectionBits : []
      StrawHitMask          : []
   }
