BEGIN_PROLOG

TrigDAQProducers : {
  artFragFromDTCEvents: {
    diagLevel: 0
    makeCRVFrag: 0
    makeCaloFrag: 1
    makeSTMFrag: 0
    makeTrkFrag: 1
