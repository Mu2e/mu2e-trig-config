BEGIN_PROLOG

TrigAprProducers : {
  TrigTZClusterFinder : { @table::CalPatRec.producers.TZClusterFinder
    chCollLabel                : "TrigFlagPH"
    ccCollLabel                : "CaloClusterFast"
  }

  TrigAprHelixFinder: { @table::CalPatRec.producers.AgnosticHelixFinder
    chCollLabel             : "TrigFlagPH"
    tcCollLabel             : "TrigTZClusterFinder"
    ccCollLabel             : "CaloClusterFast"
    minNHelixComboHits      : 10
    maxEDepAvg              : @local::TrigTrkProducers.TrigHelixFinderParams.maxEDepAvg
    maxNHitsRatio           : 1.5
    doEDepFlag              : true
    eDepFlagThresh          : 0.003
  }

  TrigAprHelixMerger: {
    HelixFinders: [
      "TrigAprHelixFinder"
    ]
    debugLevel: 0
    deltanh: 5
    module_type: "MergeHelices"
    scaleXY: 1.1
    scaleZPhi: 7.5e-1
    MinHitOverlapFraction: 0.5
    MinNHitOverlap: 10
  }

  TrigAprKSF: {
    module_type : LoopHelixFit
    MaterialSettings : { @table::Mu2eKinKalTrigger.MAT     }
    KKFitSettings    : { @table::Mu2eKinKalTrigger.KKFIT   }
    FitSettings      : { @table::Mu2eKinKalTrigger.SEEDFIT }
    ExtensionSettings: { @table::Mu2eKinKalTrigger.SEEDEXT }
    ModuleSettings   : {
      @table::Mu2eKinKalTrigger.LOOPHELIX
      FitParticle : @local::Particle.eminus
      HelixSeedCollections   : [ "TrigAprHelixMerger" ]
    }
    UsePDGCharge     : false
    FitDirection     : @local::FitDir.downstream
  }

  TrigAprUeKSF: {
    module_type : LoopHelixFit
    MaterialSettings : { @table::Mu2eKinKalTrigger.MAT     }
    KKFitSettings    : { @table::Mu2eKinKalTrigger.KKFIT   }
    FitSettings      : { @table::Mu2eKinKalTrigger.SEEDFIT }
    ExtensionSettings: { @table::Mu2eKinKalTrigger.SEEDEXT }
    ModuleSettings   : {
      @table::Mu2eKinKalTrigger.LOOPHELIX
      FitParticle : @local::Particle.eminus
      HelixSeedCollections   : [ "TrigAprHelixMerger" ]
    }
    UsePDGCharge     : false
    FitDirection     : @local::FitDir.upstream
  }

  AprTrkDe80m70pD0200TriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
  AprTrkDe80m70pTriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
  AprTrkDe50D0200TriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
  AprTrkDe80m70pD0200MultiTrkTriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
  AprTrkDe50MultiHelixTriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
  AprTrkUe80m70pTriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
  AprHelixTriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
}

END_PROLOG
