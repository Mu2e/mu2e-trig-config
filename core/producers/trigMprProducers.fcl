BEGIN_PROLOG

TrigMprProducers : {
  TrigRobustMultiHelixFinder : {
    module_type   : RobustMultiHelixFinder
    ComboHitCollection      : "TrigFlagPH"
    TimeClusterCollection   : "TrigTimeClusterFinder"
    Helicities              : [-1,1]
    MinDRCircle             : 40
    MinRadCircle            : 150
    MaxRadCircle            : 350
    MinDXY2Circle           : 1600
    ClusteringPhiBin        : 0.1
    ClusteringMinBin        : 30
    TargetCon               : false
    TargetRadius            : 150
    MinDPDZSlope            : 150
    MaxDPDZSlope            : 350
    DPDZStep                : 25
    MaxDPhiHelInit          : 1.0
    MaxDPhiHelFit           : 0.7
    MaxDTHelFit             : 25
    CaloClusterMinE         : 30
    CaloClusterWeight       : 5
    MaxChi2Hit              : 50
    MinDZTrk                : 1500
    FitCircleStrategy       : "ChiSquared"  # this is just a guess FIXME
    MinStrawHits            : 10
    NMaxTrkIter             : 3
    DiagLevel               : 0
    maxEDepAvg              : @local::TrigTrkProducers.TrigHelixFinderParams.maxEDepAvg
  }
  TrigMprHelixMergerDe: {
    HelixFinders: [ "TrigRobustMultiHelixFinder:Positive", "TrigRobustMultiHelixFinder:Negative" ]
    debugLevel: 0
    deltanh: 5
    module_type: "MergeHelices"
    scaleXY: 1.1
    scaleZPhi: 7.5e-1
    MinHitOverlapFraction: 0.5
    MinNHitOverlap: 10
  }
  TrigMprKSFDe: {
    module_type : LoopHelixFit
    MaterialSettings : { @table::Mu2eKinKalTrigger.MAT     }
    KKFitSettings    : { @table::Mu2eKinKalTrigger.KKFIT   }
    FitSettings      : { @table::Mu2eKinKalTrigger.SEEDFIT }
    ExtensionSettings: { @table::Mu2eKinKalTrigger.SEEDEXT }
    ModuleSettings   : {
      @table::Mu2eKinKalTrigger.LOOPHELIX
      FitParticle : @local::Particle.eminus
      HelixSeedCollections   : [ "TrigMprHelixMergerDe" ]
    }
    UsePDGCharge     : false
    FitDirection     : @local::FitDir.downstream
  }

  MprTrkDe80m70pD0200TriggerInfoMerger: {
    module_type: "MergeTriggerInfo"
  }
}

END_PROLOG
