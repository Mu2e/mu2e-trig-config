BEGIN_PROLOG

# Basic calo cluster selection, to be overriden
TrigCalClusterID : {
    CaloClusterModuleLabel: "CaloClusterFast"
    DiskID: [ 0, 1 ]
    MaxClusterEnergy: 200.
    MinClusterRadius: 0.
    MaxNCel: 20
    MinNCel: 1
    MinNCl: 1
    diagLevel: 0
    module_type: "CaloClusterCounterFilter"
}

TrigCalFilters:{
  CaloCosmicPS    : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloMVANNCEPS   : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloPhotonPS    : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloRMCPS       : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloCluster50PS    : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloCluster60PS    : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloCluster70PS    : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloCluster75PS    : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloCluster80PS    : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}
  CaloN0SourcePS : { module_type: PrescaleEvent
      eventModeConfig : [  { eventMode: OnSpill prescale:-1}, { eventMode: OffSpill prescale:-1} ]}

  CaloCosmicFilter: {
    CaloClusterModuleLabel: "CaloClusterFast"
    InnerRadius: 460
    MaxEnergy: 600
    MinCelEneCut: 12
    MinEnergy: 90
    MinNCrystalHits: 0
    MinNumCeldiagver: 6
    MinNumCelinout: 6
    OutRadius: 580
    debugLevel: 0
    diagmaxval: 2.5
    diagminval: 1.5
    dyminval: 140
    module_type: "CaloCosmicCalib"
  }

  CaloMVANNCEFilter: {
    caloBkgMVA: {
      MVAWeights: "Offline/CaloFilters/data/CE_NN_ReLU.weights.xml"
    }
    caloClusterCollection: "CaloClusterFast"
    diagLevel: 0
    minEtoTest: 40
    minMVAScore: 9.5e-1
    module_type: "FilterEcalNNTrigger"
  }

  CaloPhotonFilter: {
    BackgroundTemplates: "OfflineData/CaloFilters/v7_1_5/caloLH_bkg.root"
    CaloClusterModuleLabel: "CaloClusterFast"
    ClusterEnergyStep: 10
    ClusterRadialDistStep: 50
    DropSecondDisk: false
    MinClusterEnergy: 50
    MinClusterRadialDist: 350
    MinLikelihoodCut: [
      4.144e1,
      1.036e1
    ]
    SignalTemplates: "OfflineData/CaloFilters/v7_1_5/caloLH_ce.root"
    module_type: "CaloLikelihood"
  }

  CaloCluster50Filter: {
    @table::TrigCalClusterID
    MinClusterEnergy: 50.
  }

  CaloCluster60Filter: {
    @table::TrigCalClusterID
    MinClusterEnergy: 60.
  }

  CaloCluster70Filter: {
    @table::TrigCalClusterID
    MinClusterEnergy: 70.
  }

  CaloCluster75Filter: {
    @table::TrigCalClusterID
    MinClusterEnergy: 75.
  }

  CaloCluster80Filter: {
    @table::TrigCalClusterID
    MinClusterEnergy: 80.
  }

  CaloRMCFilter: {
    CaloClusterModuleLabel: "CaloClusterFast"
    DiskID: [
      0, 1
    ]
    MaxClusterEnergy: 200
    MaxNCel: 10
    MinClusterEnergy: 80
    MinClusterRadius: 490
    MinNCel: 2
    MinNCl: 1
    diagLevel: 0
    module_type: "CaloClusterCounterFilter"
  }

  CaloN0SourceFilter: {
      caloDigiCollection: "CaloDigiMaker"
      maxCaloEnergy: -1
      maxNCaloDigi: 5000
      maxNStrawDigi: -1
      minNCaloDigi: -1
      minNStrawDigi: -1
      module_type: "DigiFilter"
      strawDigiCollection: "notUsed"
      triggerPath: "minBiasCaloDigiCount"
      useCaloDigi: true
      useStrawDigi: false
   }

}

END_PROLOG
